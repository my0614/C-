using System;
using System.Collections;
using System.Threading.Tasks.Dataflow;
using static System.Console;

namespace _0703
{
    class Program
    {
        static void Main(string[] args)
        {
             WriteLine("구구단 몇단을 하시겠습니까?");
            string a = ReadLine();
            int value = Convert.ToInt32(a);
            for(int i=1;i<=9;i++)
            {
                int result = i * value;
                WriteLine("{0} * {1} = {2}",value,i, result);
            }


        }
    }
}
